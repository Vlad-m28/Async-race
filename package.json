        "css-loader": "^6.11.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "html-webpack-plugin": "^5.6.3",
        "mini-css-extract-plugin": "^2.9.2",
        "prettier": "^2.4.1",
        "style-loader": "^3.3.4",
        "ts-loader": "^9.5.2",
        "typescript": "^4.9.5",
